@model GSTEducationERPLibrary.Counsellor.Counsellor

<style>
    .single-line-ellipsis {
        white-space: nowrap;
        overflow: hidden;
        text-overflow: ellipsis;
        max-width: 200px; /* Adjust as necessary */
    }

    .popover-header {
        background-color: #33CCCC;
        color: /white/ black;
        border-bottom: 1px solid #33CCCC;
    }
</style>
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>
                Sr.No
            </th>

            <th>
                @Html.DisplayNameFor(model => model.CandidateCode)
            </th>

            <th>
                @Html.DisplayNameFor(model => model.FullName)
            </th>

            <th>
                @Html.DisplayNameFor(model => model.EmailId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ContactNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EnquiryFor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AdmmissionDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MonthYear)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FollowUpNote)
            </th>






        </tr>
    </thead>

    <tbody>
        @foreach (var item in Model.lstforDropoutStudents)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.SerialNo)
                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.CandidateCode)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FullName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.EmailId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ContactNumber)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.EnquiryFor)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.AdmmissionDate)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.MonthYear)
                </td>
                @*<td>
                        @Html.DisplayFor(modelItem => item.FollowUpNote)
                    </td>*@
                <td class="single-line-ellipsis" data-toggle="popover" title="FollowUpNote" data-content="@item.FollowUpNote">@Html.DisplayFor(modelItem => item.FollowUpNote)</td>

            </tr>
        }
    </tbody>

</table>

<script src="~/Content/plugins/jquery/jquery.min.js" type="text/javascript"></script>
<script src="~/Content/Trainer/js/CourseContent.js"></script>
<script src="~/Content/Admin/js/Icon.js"></script>
