            </tr>
        </thead>
        <tbody>
            @if (Model.lstTimetableAp != null && Model.lstTimetableAp.Any())
            {
                foreach (var item in Model.lstTimetableAp)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.SerialNo)
                        </td>
                        <td>@Html.DisplayFor(modelItem => item.SectionName)</td>
                        <td>@Html.DisplayFor(modelItem => item.TopicName)</td>
                        <td>@Html.DisplayFor(modelItem => item.StaffName)</td>
                        <td>@Html.DisplayFor(modelItem => item.LabName)</td>
                        <td>
                            @{
                                var durationParts = item.Duration.Split(' ');
                                var daysPart = durationParts.Length > 0 ? durationParts[0] + " " + durationParts[1] : item.Duration;
                            }
                            @daysPart
                        </td>
                        <td>@Html.DisplayFor(modelItem => item.StartDate)</td>
                        <td>@Html.DisplayFor(modelItem => item.EndDate)</td>
                    </tr>
                }
            }
            else
            {
                <tr>
                    <td colspan="10">No Data Available</td> <!-- Adjusted colspan to match number of columns -->
                </tr>
            }
        </tbody>
    </table>


